{
    "name": "tea/future",
    "description": "Polyfill backporting some PHP 5.4+, 5.5+ ,5.6+, 7.0+ and 7.1+ features to lower PHP versions.",
    "keywords": ["polyfill", "shim", "compatibility", "portable"],
    "homepage": "https://github.com/php-tea-lab/future",
    "license": "MIT",
    "authors": [
        {
            "name": "David Kyalo",
            "email": "davidmkyalo@gmail.com",
            "homepage": "http://www.davidkyalo.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/polyfill-php54": "~1.3.0",
        "symfony/polyfill-php55": "~1.3.0",
        "symfony/polyfill-php56": "~1.3.0",
        "symfony/polyfill-php70": "~1.3.0",
        "symfony/polyfill-php71": "~1.3.0"
    },
    "support": {
        "issues": "https://github.com/php-tea-lab/future/issues",
        "source": "https://github.com/php-tea-lab/future"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.0-dev"
        }
    },
    "minimum-stability": "dev"
}
